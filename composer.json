{
  "name": "pekral/pint-rules",
  "description": "Pint - custom rule set",
  "license": ["MIT"],
  "require": {
    "php": "^8.3",
    "laravel/pint": "^1.18"
  }
,
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
